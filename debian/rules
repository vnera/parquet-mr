#!/usr/bin/make -f
export COMPONENT_HASH=9fc3dcfb34099f1c0d35af813f599ed3f34dce79
 PARQUET_VERSION=1.9.0+cdh6.3.2
 PARQUET_PATCHED_VERSION=1.9.0-cdh6.3.2
 PARQUET_BASE_VERSION=1.9.0
 PARQUET_RELEASE=1605554
 CDH_CUSTOMER_PATCH=p0
 CDH_PARCEL_CUSTOM_VERSION=1.9.0+cdh6.3.2-1605554.bionic
